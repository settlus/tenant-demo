{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react",
        "<THIRD_PARTY_MODULES>",
    
        "^~/api/(.*)$",
        "^~/components/(.*)$",
        "^~/utils/(.*)$",
    
        "^[./]"
      ],
      "importOrderSeparation": true
}
